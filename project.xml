<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta 
    title="TerryLib Compile" 
    package="com.terrylib.terrylibcompile" 
    version="1.0.0" 
    company="Your name here" 
  />

	<app 
      main="terrylib.Load"
      path="bin" 
      file="newproject" 
   />
   
	<window
      width="800" height="300" unless="mobile" 
   ></window>
   
	<window
      fps="60"
      background="0x000000"
      orientation="landscape"
      hardware="true"
      vsync="true"
   ></window>
   
	<window
      antialiasing="0" 
      if="cpp"
   ></window>
   
	<source path="src" />
	
	<set name="openfl-legacy" unless="flash"/>
	<haxelib name="openfl" />
	
	<assets path="data" exclude="*.svg|*.mp3|*.ogg|*.wav" />
	<icon path="data/icon/power.svg" />
	
</project>